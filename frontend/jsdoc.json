{
  "source": {
    "include": ["./src/"],
    "exclude": ["./src/serviceWorker.js", "./src/setupTests.js"],
    "includePattern": "\\.(js|jsx)$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "markdown": {
    "parser": "gfm"
  }
} 